import java.util.*;

public class Main {
	
	public static void main(String[] args) {
		// Write your code here
		Scanner sc = new Scanner(System.in);
        int Number=sc.nextInt();
        int oddsum=0,evensum=0;
        while(Number>0)
        {
            int rem=Number%10;
            if (rem%2==0)
            {
                evensum=evensum+rem;
            }
            else
            {
                oddsum=oddsum+rem;
            }
            Number=Number/10;
        }
        System.out.println(evensum+" "+oddsum);
        
	}
}
